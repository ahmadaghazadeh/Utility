<resources>


    <!-- Custom Data Binding attr . -->
    <declare-styleable name="ImageView">
        <attr name="resId" format="reference" />
       <attr name="imageUrl" format="string" />
        <attr name="error" format="reference" />
        <attr name="paletteResId" format="reference" />
    </declare-styleable>

    <attr name="addTextChangedListener" format="string" />
    <attr name="is_app_default_Font" format="boolean" />
    <attr name="text_font" format="string" />
    <attr name="backgroundColorName" format="string" />

    <declare-styleable name="TextView">
        <attr name="text_font" />
        <attr name="is_app_default_Font" />
        <attr name="backgroundColorName" />
    </declare-styleable>
    <declare-styleable name="EditText">
        <attr name="addTextChangedListener" />
        <attr name="text_font"  />
        <attr name="is_app_default_Font" />
    </declare-styleable>





    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


